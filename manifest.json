{
  "manifest_version": 2,

  "name": "JS Injector",
  "description": "Inject javascript to the webpage you visit",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
   "activeTab",
   "storage",
   "tabs",
   "activeTab",
   "http://*/",
   "https://*/"
   ]
}
